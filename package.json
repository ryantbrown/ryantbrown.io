        "css:prod": "npx tailwindcss -i src/style.css -o dist/style.css --minify",
        "build": "npm run css:prod && esno build/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ryantbrown/ryantbrown.io.git"
    },
    "devDependencies": {
