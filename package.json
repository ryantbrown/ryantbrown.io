        "@types/imagemin-jpegtran": "^5.0.1",
        "@types/imagemin-pngquant": "^8.0.0",
        "dotenv": "^10.0.0",
        "esno": "^0.13.0",
        "fs-extra": "^10.0.0",
        "imagemin": "^8.0.1",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-pngquant": "^9.0.2",
        "rimraf": "^3.0.2",
        "tailwindcss": "^3.0.12",
        "tinify": "^1.6.1"
    },
    "dependencies": {
        "html-minifier": "^4.0.0"
    }
}
